    "lint-staged": "^15.2.2",
    "normalize.css": "^8.0.1",
    "prettier": "^3.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.23.0",
    "react-scripts": "5.0.1",
    "redux": "^5.0.1",
